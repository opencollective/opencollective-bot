{
  "name": "opencollective-bot",
  "version": "0.0.0-semantic-release",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "dev": "nodemon --exec dotenv ts-node src/index.ts",
    "test": "NODE_ENV=test jest",
    "coverage": "yarn codecov",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "deploy:staging": "now -e APP_ID=@oc-bot-staging-app-id -e WEBHOOK_SECRET=@oc-bot-staging-webhook-secret -e PRIVATE_KEY=@oc-bot-staging-private-key -e SENTRY_DSN=@oc-bot-staging-sentry-dsn --target staging && now alias bot-staging.opencollective.com",
    "deploy:production": "now --target production"
  },
  "engines": {
    "node": "10.x"
  },
  "dependencies": {
    "@hapi/joi": "16.1.7",
    "body-parser": "1.19.0",
    "detect-indent": "6.0.0",
    "js-yaml": "3.13.1",
    "lodash": "4.17.15",
    "multilines": "1.0.2",
    "node-fetch": "2.6.0",
    "probot": "9.6.4"
  },
  "devDependencies": {
    "@now/node-server": "0.7.4",
    "@types/body-parser": "1.17.1",
    "@types/btoa": "1.2.3",
    "@types/express": "4.17.2",
    "@types/hapi__joi": "16.0.3",
    "@types/jest": "24.0.23",
    "@types/js-yaml": "3.12.1",
    "@types/lodash": "4.14.149",
    "@types/nock": "10.0.3",
    "@types/node": "12.12.9",
    "@types/node-fetch": "2.5.3",
    "@types/request-promise-native": "1.0.17",
    "btoa": "1.2.1",
    "codecov": "3.6.1",
    "dotenv-cli": "3.0.0",
    "husky": "3.1.0",
    "jest": "24.9.0",
    "nock": "11.7.0",
    "nodemon": "1.19.4",
    "npm-run-all": "4.1.5",
    "prettier": "1.19.1",
    "pretty-quick": "2.0.1",
    "request": "2.88.0",
    "request-promise-native": "1.0.8",
    "rimraf": "3.0.0",
    "smee-client": "1.1.0",
    "ts-jest": "24.1.0",
    "ts-node": "8.5.2",
    "typescript": "3.7.2"
  },
  "license": "MIT"
}
