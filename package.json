    "nock": "10.0.6",
    "npm-run-all": "4.1.5",
    "prettier": "1.16.4",
    "pretty-quick": "1.8.0",
    "request-promise-native": "1.0.7",
    "rimraf": "2.6.3",
    "ts-jest": "24.0.0",
    "ts-node": "7.0.1",
    "typescript": "3.3.3333"
